{
  "name": "gulp-es6-boilerplate",
  "version": "0.1.0",
  "description": "Gulp ES6 Boilerplate",
  "main": "gulpfile.babel.js",
  "scripts": {
    "install:info": "npm install --loglevel=info",
    "start": "gulp",
    "test": "gulp lint && gulp test",
    "lint": "gulp lint",
    "david": "david",
    "david:update": "david update",
    "karma": "karma start karma.conf.js --log-level debug --single-run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release:patch": "npm run changelog && git add CHANGELOG.md && git commit -m \"docs(changelog): update\" && npm version patch -m \"chore(release): Bump to %s\"",
    "release:minor": "npm run changelog && git add CHANGELOG.md && git commit -m \"docs(changelog): update\" && npm version minor -m \"chore(release): Bump to %s\"",
    "release:major": "npm run changelog && git add CHANGELOG.md && git commit -m \"docs(changelog): update\" && npm version major -m \"chore(release): Bump to %s\"",
    "postversion": "git push --follow-tags",
    "gulp:dev": "gulp dist --env develop",
    "gulp:stag": "gulp dist --env staging",
    "gulp:prod": "gulp dist --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugofqueiros/gulp-es6-boilerplate.git"
  },
  "keywords": [
    "gulp",
    "babel",
    "gulp babel",
    "starter",
    "boilerplate",
    "es6",
    "browserify",
    "simple"
  ],
  "author": "Hugo QueirÃ³s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hugofqueiros/gulp-es6-boilerplate/issues"
  },
  "homepage": "https://github.com/hugofqueiros/gulp-es6-boilerplate#readme",
  "engines": {
    "node": ">=0.12"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "connect-logger": "0.0.1",
    "conventional-changelog-cli": "^1.1.1",
    "cssnano": "^3.5.2",
    "david": "^8.0.0",
    "del": "^2.2.0",
    "doiuse": "^2.3.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.0",
    "gulp-csso": "^2.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-file-include": "^0.14.0",
    "gulp-filter": "^4.0.0",
    "gulp-gzip": "^1.2.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-image-resize": "^0.10.0",
    "gulp-imagemin": "^3.0.2",
    "gulp-less": "^3.0.5",
