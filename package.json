{
  "name": "gulp-es6-boilerplate",
  "version": "0.0.4",
  "description": "Gulp ES6 Boilerplate",
  "main": "gulpfile.babel.js",
  "scripts": {
    "install:info": "npm install --progress=false --loglevel=info",
    "start": "gulp",
    "test": "gulp lint && gulp test",
    "lint": "gulp lint",
    "david": "david",
    "david:update": "david update",
    "karma": "karma start karma.conf.js --log-level debug --single-run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release:patch": "npm run changelog && npm version patch -m \"chore(release): Bump to %s\"",
    "release:minor": "npm run changelog && npm version minor -m \"chore(release): Bump to %s\"",
    "release:major": "npm run changelog && npm version major -m \"chore(release): Bump to %s\"",
    "postversion": "git push --follow-tags",
    "gulp:dev": "gulp dist --env develop",
    "gulp:stag": "gulp dist --env staging",
    "gulp:prod": "gulp dist --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugofqueiros/gulp-es6-boilerplate.git"
  },
  "keywords": [
    "gulp",
    "babel",
    "gulp babel",
    "starter",
    "boilerplate",
    "es6",
    "browserify",
    "simple"
  ],
  "author": "Hugo QueirÃ³s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hugofqueiros/gulp-es6-boilerplate/issues"
  },
  "homepage": "https://github.com/hugofqueiros/gulp-es6-boilerplate#readme",
  "engines": {
    "node": ">=0.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "connect-logger": "0.0.1",
    "conventional-changelog-cli": "^1.1.1",
    "cssnano": "^3.5.2",
    "david": "^7.0.1",
    "del": "^2.2.0",
    "eslint": "^2.3.0",
    "eslint-config-gigigo": "1.2.3",
