    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rev": "^7.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylelint": "^3.0.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-tar": "^1.8.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.0.7",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^5.0.0",
    "karma": "^1.2.0",
    "karma-mocha": "^1.1.1",
    "mocha": "^3.0.2",
    "object-assign": "^4.0.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-less": "^0.14.0",
    "postcss-reporter": "^1.3.3",
    "postcss-svgo": "^2.1.2",
    "pretty-hrtime": "^1.0.2",
    "require-dir": "^0.3.0",
    "rimraf": "^2.5.2",
    "rsyncwrapper": "^1.0.0",
    "run-sequence": "^1.1.5",
